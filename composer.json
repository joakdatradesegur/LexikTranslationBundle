{
    "name": "tradesegur/translation-bundle",
    "type": "symfony-bundle",
    "description": "Fork from Lexik Translation Bundle (lexik/translation-bundle). Thanks to Cedric Girard and Dev Lexik.",
    "keywords": ["Symfony4", "Symfony 5", "bundle", "translation", "i18n"],
    "homepage": "https://github.com/joakdatradesegur/LexikTranslationBundle",
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "JoaquÃ­n Alejandro Duro Arribas",
            "email": "dual@tradesegur.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^7.3",
        "symfony/framework-bundle": "~4.3|| ~5.0",
        "symfony/translation": "^v4.3.0| ^v5.0.0",
        "symfony/yaml": "^v4.3.0 || ^v5.0.0",
        "symfony/event-dispatcher": "^v4.3.0 || ^v5.0.0",
        "ext-json": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "doctrine/orm": "^2.2.3",
        "doctrine/doctrine-bundle": "^2.0.10",
        "doctrine/data-fixtures": "^1.5.0",
        "phpunit/phpunit": "^7.5.20"
    },
    "suggest": {
        "doctrine/orm": ">=2.4"
    },
    "autoload":     {
        "psr-4": { "Lexik\\Bundle\\TranslationBundle\\": "" }
    },
    "autoload-dev": {
        "classmap": [
            "Tests/app/AppKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x.x-dev"
        }
    },
    "minimum-stability": "alpha"
}
